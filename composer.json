{
    "name": "spoje.net/flexplorer",
    "description": "Průzkumník a editor evidencí FlexiBee",
    "version": "0.5",
    "license": "GPL-2.0",
    "type": "project",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }   
    ],
    "require": {
        "php": ">= 5.5.9",
        "spoje.net/flexipeehp": "*",
        "vitexsoftware/ease-framework": "*"
    },
    "autoload": {
        "psr-4": {
            "Flexplorer\\": "src/classes/",
            "Flexplorer\\ui\\": "src/classes/ui/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Flexplorer\\": "testing/PHPUnit/src/classes/",
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\FlexiPeeHP\\": "vendor/spoje.net/flexipeehp/testing/src/FlexiPeeHP/"
        }
    },
    "require-dev": {
        "phpunit/php-token-stream": "~1.2",
        "phpunit/phpunit": "5.3.*",
        "phpunit/phpunit-skeleton-generator": "*"
    }

}
