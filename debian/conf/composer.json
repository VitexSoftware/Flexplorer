{
    "name": "spoje.net/flexplorer",
    "description": "Průzkumník a editor evidencí FlexiBee",
    "version": "0.1",
    "type": "project",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "require": {
        "php": ">= 5.5.9",
        "spoje.net/flexipeehp": "dev-master",
        "vitexsoftware/ease-framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Flexplorer\\": "web/classes/",
            "Flexplorer\\ui\\": "web/classes/ui/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        }
    ]

}
