{
    "name": "vitexsoftware/flexplorer",
    "description": "Průzkumník a editor evidencí FlexiBee",
    "version": "1.1",
    "type": "project",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
	"vendor-dir": "/var/lib/flexplorer"
    },
    "require": {
        "php": ">= 5.5.9",
        "flexipeehp": "*",
        "ease-framework": "*",
        "league/commonmark": "^0.15.0"        
    },
    "autoload": {
        "psr-4": {
            "Flexplorer\\": "classes/",
            "Flexplorer\\ui\\": "classes/ui/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        }
    ]

}
